{
  "version": "2.0",
  "extensions": {
    "http": {
      "customHeaders": {
        "Content-Type": "application/json; charset=utf-8"
      }
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  }
}